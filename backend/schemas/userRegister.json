{
  "$schema": "http://json-schema.org/draft-01/schema",
  "$id": "https://greenthumb.com/userRegister.schema.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "zone": {
      "type": "integer",
      "minimum": 1,
      "maximum": 13
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    }
  },
  "additionalProperties": false,
  "required": ["username", "password", "zone", "email"]
}
